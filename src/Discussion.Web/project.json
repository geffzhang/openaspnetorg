{
  "buildOptions": {
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "debugType": "portable"
  },
  "webroot": "wwwroot",

  "dependencies": {
    "Microsoft.NETCore.Platforms": "1.0.1-rc2-24027",
    "Jusfr.Persistent": "2.0.0-rc2",
    "Markdown": "1.14.7-beta",
    "RavenDB.Client": "3.5.35129-Unstable",
    "Microsoft.AspNetCore.Diagnostics": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Mvc": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0-rc2-final",
    "Microsoft.Extensions.Configuration.Json": "1.0.0-rc2-final",
    "Microsoft.Extensions.Logging.Debug": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Mvc.TagHelpers": "1.0.0-rc2-final",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.0.0-rc2-final",
    "Microsoft.Extensions.Configuration.CommandLine": "1.0.0-rc2-final",
    "Microsoft.Extensions.PlatformAbstractions": "1.0.0-rc2-final",
    "System.Security.Claims": "4.0.1-rc2-24027",
    "Microsoft.AspNetCore.Authorization": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Authentication.Cookies": "1.0.0-rc2-final",
    "Microsoft.AspNetCore.Http.Abstractions": "1.0.0-rc2-final"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": [ "dnxcore50", "netcore50" ],
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0-rc2-3002702"
        }
      }
    }
  },
  "runtimes": {
    "centos.7-x64": { },
    "ubuntu.14.04-x64": { },
    "osx.10.11-x64": { },
    "win8-x64": { }
  },

  "exclude": [
    "wwwroot",
    "node_modules",
    "tmp_wwwroot"
  ],
  "publishOptions": {
    "include": [
      "logs",
      "Views",
      "wwwroot",
	  "appsettings.QA.json",
	  "appsettings.Integration.json",
	  "appsettings.Production.json"
    ]
  },
  "publishExclude": [
    "tmp_wwwroot/**/*",
    "wwwroot/.idea",
    "wwwroot/node_modules",
    "wwwroot/lib/source",
    "wwwroot/stylesheets/scss",
    "wwwroot/scripts/es6",
    "wwwroot/.bowerrc",
    "wwwroot/bower.json",
    "wwwroot/gulpfile.js",
    "wwwroot/package.json",
    "Properties/launchSettings.json",
    "**.user",
    "**.vspscc"
  ]
}
